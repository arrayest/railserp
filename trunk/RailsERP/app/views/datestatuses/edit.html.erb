<h1><%= t 'datestatus_edit_headline' %></h1>

<%= error_messages_for :datestatus %>

<% form_for(@datestatus) do |f| %>
  <p>
    <b><%= t 'datestatus_editnewshowindex_status' %></b><br />
    <%= f.text_field :status %>
  </p>

  <p>
    <%= f.submit t('datestatus_editnewshowindex_update') %>
  </p>
<% end %>

<%= link_to t('datestatus_editnewshowindex_show'), @datestatus %> |
<%= link_to t('datestatus_editnewshowindex_back'), datestatuses_path %>
