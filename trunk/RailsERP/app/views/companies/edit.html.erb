<h1>Editing company</h1>

<%= error_messages_for :company %>

<% form_for(@company) do |f| %>
  <p>
    <b>Name</b><br />
    <%= f.text_field :name %>
  </p>

  <p><b>Companyart</b><br />
    <%= f.collection_select :companyart_id ,@companyarts, :id, :name %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @company %> |
<%= link_to 'Back', companies_path %>
