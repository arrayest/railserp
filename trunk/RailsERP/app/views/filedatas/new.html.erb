<h1>New filedata</h1>

<%= error_messages_for :filedata %>

<% form_for(@filedata, :html => { :multipart => true }) do |f| %>
  <p>
    <b>File</b><br />
    <%= f.file_field :file %>
  </p>

  <p>
    <b>Info</b><br />
    <%= f.text_area :info %>
  </p>

  <p>
    <b>Contact</b><br />
    <%= f.collection_select :contact_id ,@contacts, :id, :name %>
  </p>

  <p>
    <b>Folder</b><br />
    <%= f.collection_select :folder_id ,@folders, :id, :name %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', filedatas_path %>
