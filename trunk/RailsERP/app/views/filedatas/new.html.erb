<h1><%= t 'filedatas_new_headline' %></h1>

<%= error_messages_for :filedata %>

<% form_for(@filedata, :html => { :multipart => true }) do |f| %>
  <p>
    <b><%= t 'filedatas_editnewshowindex_filename' %></b><br />
    <%= f.file_field :file %>
  </p>

  <p>
    <b><%= t 'filedatas_editnewshowindex_info' %></b><br />
    <%= f.text_area :info %>
  </p>

  <p>
    <b><%= t 'filedatas_editnewshowindex_contact' %></b><br />
    <%= f.collection_select :contact_id ,@contacts, :id, :name %>
  </p>

  <p>
    <b><%= t 'filedatas_editnewshowindex_folder' %></b><br />
    <%= f.collection_select :folder_id ,@folders, :id, :name %>
  </p>

  <p>
    <%= f.submit t('filedatas_editnewshowindex_create') %>
  </p>
<% end %>

<%= link_to t('filedatas_editnewshowindex_back'), filedatas_path %>
